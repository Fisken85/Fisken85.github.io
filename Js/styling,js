function setHeading() {
    let firstname = document.getElementById("firstname")
    console.log(firstname)
    let heading =document.getElementById("heading")
    heading.innerText = "Velkommen, " + firstname.value;
    heading.style.color = "red"
}
let favtal = 4
console.log(favtal)
let number1 = 10;
